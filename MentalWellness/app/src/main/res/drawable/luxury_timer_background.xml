<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#F0FFFFFF" />
            <stroke android:width="2dp" android:color="#D4AF37" />
        </shape>
    </item>
    <item android:top="4dp" android:left="4dp" android:right="4dp" android:bottom="4dp">
        <shape android:shape="oval">
            <gradient
                android:type="radial"
                android:gradientRadius="100%p"
                android:startColor="#FFFFFF"
                android:endColor="#F8F0DF" />
            <stroke android:width="1dp" android:color="#99D4AF37" />
        </shape>
    </item>
    <item android:top="12dp" android:left="12dp" android:right="12dp" android:bottom="12dp">
        <shape android:shape="oval">
            <stroke android:width="2dp" android:color="#33D4AF37" />
        </shape>
    </item>
</layer-list> 